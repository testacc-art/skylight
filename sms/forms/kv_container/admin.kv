        padding: dp(10)

        Label:
            size_hint_x: .6

        BoxLayout:
            orientation: 'vertical'
            size_hint_x: .4
            MenuButton:
                text:"Accounts"
                on_press:
                    root.manager.current = 'accounts'
            MenuButton:
