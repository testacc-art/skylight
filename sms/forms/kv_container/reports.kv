<Reports>:
    tabbed_panel: tabbed_panel
    BoxLayout:
        orientation: 'vertical'
        BoxLayout:
            size_hint_y: None
            height: 50
            canvas.before:
                Color:
                    rgba: (0,0,0,.4)
                Rectangle:
                    size: self.size
                    pos: self.pos
            Label:
                text: 'Reports'
                font_size: 20
                halign: 'center'
                valign: 'middle'
                markup: True
                size_hint_x: .9

            ImageButton:
                size_hint_x: .1
                source: os.path.join(root.form_root, 'resc', 'icons', 'icons8-back-64.png')
                on_press:
                    app.root.transition.direction = 'right'
                    app.root.current = 'main_page'
        TabbedPanel:
            id: tabbed_panel
